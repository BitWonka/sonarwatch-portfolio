{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.js"],
      "rules": {}
    },
    {
      "files": ["*.ts"],
      "rules": {}
    },
    {
      "files": ["*.js"],
      "rules": {}
    },
    {
      "files": ["**/services.ts"],
      "rules": {
        "@typescript-eslint/no-unused-vars": ["warn", { "varsIgnorePattern": "^services$" }]
      }
    },
    {
      "files": ["./package.json", "./generators.json"],
      "parser": "jsonc-eslint-parser",
      "rules": {
        "@nx/nx-plugin-checks": "error"
      }
    }
  ]
}
